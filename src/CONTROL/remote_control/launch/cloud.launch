<!-- -*- mode: XML -*- -->
<launch>
  <arg name="racecar_version" default="racecar-v2" />
  <arg name="run_camera" default="false"/>


  <include file="$(find racecar)/launch/includes/autonomous-teleop.launch.xml">
    <arg name="run_camera" value="$(arg run_camera)" />
  </include>

  <node name="talker" pkg="remote_control" type="talker" output="screen">
  </node>

</launch>